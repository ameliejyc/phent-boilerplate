{
  "name": "boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "tape tests/*.test.js | tap-spec",
    "start": "node ./bin/www",
    "devStart": "npm run server-watch & npm run css-watch",
    "server-watch": "nodemon ./bin/www",
    "css-watch": "postcss ./stylesheets/index.css --output ./public/style.min.css --watch --config ./stylesheets/postcss.config.js",
    "css-build": "postcss ./stylesheets/index.css --output ./public/style.min.css --config ./stylesheets/postcss.config.js",
    "nyc": "nyc npm test && nyc report --reporter=html && nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "codecov": "^2.3.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "env2": "^2.2.0",
    "express": "~4.15.2",
    "express-handlebars": "^3.0.0",
    "hbs": "~4.0.1",
    "morgan": "~1.8.1",
    "pg": "^7.3.0",
    "serve-favicon": "~2.4.2",
    "tachyons-custom": "^4.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "eslint": "^4.7.2",
    "eslint-config-semistandard": "^11.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "nock": "^9.0.20",
    "nodemon": "^1.12.1",
    "nyc": "^11.2.1",
    "postcss-clean": "^1.1.0",
    "postcss-cli": "^4.1.1",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^6.1.0",
    "postcss-import": "^11.0.0",
    "supertest": "^3.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0"
  }
}
